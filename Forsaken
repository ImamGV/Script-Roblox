local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Bacon-Library/refs/heads/main/bacon.txt"))()
local LibESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Script/main/ESP"))()

local Tabs = {
Main = Library:CreateWindow("Main"),
Visual = Library:CreateWindow("Visual"),
Add = Library:CreateWindow("Add+"),
Premium = Library:CreateWindow("Free Premium")
}
local function InfinityStamina(v)
Work = v
local StaminaScript = require(game.ReplicatedStorage.Systems.Character.Game.Sprinting)
InfStamina = game:GetService("RunService").Heartbeat:Connect(function()
if not Work then
InfStamina:Disconnect()
StaminaScript.StaminaLossDisabled = nil
return
end
StaminaScript.StaminaLossDisabled = function() 
end
end)
end
Library:CreateToggle(Tabs.Main, "Infinity Stamina", false, function(v)
InfinityStamina(v)
end)
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _G.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 24 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 24
end
end)
game.Players.LocalPlayer.CharacterAdded:Connect(function()
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _G.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 24 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 24
end
end)
end)
Library:CreateToggle(Tabs.Main, "Disabled Slow & Stun", false, function(v)
_G.NoStun = V
end)
Library:CreateToggle(Tabs.Main, "Speed Boost (Fair)", false, function(v)
_G.SpeedBoost = v
while _G.SpeedBoost do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame += game.Players.LocalPlayer.Character.Humanoid.MoveDirection * 0.25
game.Players.LocalPlayer.Character.HumanoidRootPart.CanCollide = true
wait()
end
end)
Library:CreateToggle(Tabs.Main, "Fullbright", false, function(v)
_G.Fullbright = v
_G.NoFog = v
game:GetService("RunService").RenderStepped:Connect(function()
if not game.Lighting:GetAttribute("FogStart") then game.Lighting:SetAttribute("FogStart", game.Lighting.FogStart) end
if not game.Lighting:GetAttribute("FogEnd") then game.Lighting:SetAttribute("FogEnd", game.Lighting.FogEnd) end
game.Lighting.FogStart = _G.NoFog and 0 or game.Lighting:GetAttribute("FogStart")
game.Lighting.FogEnd = _G.NoFog and math.huge or game.Lighting:GetAttribute("FogEnd")
local fog = game.Lighting:FindFirstChildOfClass("Atmosphere")
if fog then
if not fog:GetAttribute("Density") then fog:SetAttribute("Density", fog.Density) end
fog.Density = _G.NoFog and 0 or fog:GetAttribute("Density")
end
if _G.Fullbright then
game.Lighting.OutdoorAmbient = Color3.new(1,1,1)
game.Lighting.Brightness = 0
game.Lighting.GlobalShadows = false
else
game.Lighting.OutdoorAmbient = Color3.fromRGB(55,55,55)
game.Lighting.Brightness = 0
game.Lighting.GlobalShadows = true
end
end)
end)
Library:CreateToggle(Tabs.Main, "Auto Generator [BETA]", false, function(v)
_G.AutoGenerator = v
while _G.AutoGenerator == true and game.Players.LocalPlayer.Character.Humanoid.Health > 0 do
for _,v in ipairs(workspace.Map.Ingame.Map:GetChildren()) do
if v.Name == "Generator" then
v.Remotes.RE:FireServer()
end
end
wait()
end
end)

Library:CreateToggle(Tabs.Visual, "Killer ESP", false, function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v:IsA("Model") and v.Parent.Name == "Killers" and not v:FindFirstChild("Killer_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
elseif v.Parent.Name == "Killers" and not v:FindFirstChild("Killer_ESP") then
LibESP:AddESP(v.HumanoidRootPart, "Killer", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end
KillersESP = workspace.Players.DescendantAdded:Connect(function(v)
if v:IsA("Model") and v.Parent.Name == "Killers" and not v:FindFirstChild("Killers_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
elseif v.Parent.Name == "Killers" and not v:FindFirstChild("Killer_ESP") then
LibESP:AddESP(v.HumanoidRootPart, "Killer", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end)
else
KillersESP:Disconnect()
LibESP:Delete("Killers_ESP")
end
end)
Library:CreateToggle(Tabs.Visual, "Survivors ESP", false, function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v:IsA("Model") and v.Parent.Name == "Survivors" and not v:FindFirstChild("Survival_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end
SurvivalESP = workspace.Players.DescendantAdded:Connect(function(v)
if v:IsA("Model") and v.Parent.Name == "Survivors" and not v:FindFirstChild("Survival_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end)
else
SurvivalESP:Disconnect()
LibESP:Delete("Survival_ESP")
end
end)
Library:CreateToggle(Tabs.Visual, "Items ESP", false, function(v)
if v then
for _, v in ipairs(workspace:GetDescendants()) do
if v:IsA("Model") and v.Name == "BloxyCola" and not v:FindFirstChild("Items_ESP") then
LibESP:AddESP(v, "Bloxy Cola", Color3.fromRGB(255, 255, 100), 14, "Items_ESP")
elseif v:IsA("Model") and v.Name == "Medkit" and not v:FindFirstChild("Items_ESP") then
LibESP:AddESP(v, "Medkit", Color3.fromRGB(255, 0, 0), 14, "Items_ESP")
end
end
ItemsESP = workspace.DescendantAdded:Connect(function(v)
if v:IsA("Model") and v.Name == "BloxyCola" and not v:FindFirstChild("Items_ESP") then
LibESP:AddESP(v, "Bloxy Cola", Color3.fromRGB(255, 255, 100), 14, "Items_ESP")
elseif v:IsA("Model") and v.Name == "Medkit" and not v:FindFirstChild("Items_ESP") then
LibESP:AddESP(v, "Medkit", Color3.fromRGB(255, 0, 0), 14, "Items_ESP")
end
end)
else
ItemsESP:Disconnect()
LibESP:Delete("Items_ESP")
end
end)
Library:CreateToggle(Tabs.Visual, "Generator ESP", false, function(v)
if v then
for _, v in ipairs(workspace:GetDescendants()) do
if v:IsA("Model") and v.Name == "Generator" and not v:FindFirstChild("Generator_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 220, 0), 14, "Generator_ESP")
end
end
GeneratorESP = workspace.DescendantAdded:Connect(function(v)
if v:IsA("Model") and v.Name == "Generator" and not v:FindFirstChild("Generator_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 220, 0), 14, "Generator_ESP")
end
end)
else
GeneratorESP:Disconnect()
LibESP:Delete("Generator_ESP")
end
end)

Library:CreateToggle(Tabs.Add, "Jump", false, function(v)
_G.Jump = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.Jump then
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 68
else
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0
end
end)
end)

Library:CreateToggle(Tabs.Premium, "Fly Gui", false, function(v)
if v then
loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Bacon-Library/refs/heads/main/Fly Gui"))()
else
for _,v in ipairs(game.CoreGui:GetDescendants()) do
if v.Name == "FlyGui" then
v:Destroy()
end
end
end
end)
Library:CreateToggle(Tabs.Premium, "No Accessories", false, function(v)
local Char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
local HRP = Char:FindFirstChild("HumanoidRootPart")
if HRP then
if v then
if not PHP then
PHP = HRP.CustomPhysicalProperties
end
HRP.CustomPhysicalProperties = PhysicalProperties.new(100, originalPHProps.Friction, originalPHProps.Elasticity, originalPHProps.FrictionWeight, originalPHProps.ElasticityWeight)
else
if PHP then
HRP.CustomPhysicalProperties = PHP
PHP = nil
end
end
end
end)
local ScreenGui = Instance.new("ScreenGui", game:GetService("CoreGui"))
ScreenGui.Name = "Toggle"
ScreenGui.ResetOnSpawn = false
local Frame = Instance.new("Frame", ScreenGui)
Frame.Position = UDim2.new(0.875, 0, 0.29, 0)
Frame.BackgroundColor3 = Color3.new(0,0,0)
Frame.BackgroundTransparency = 0
Frame.Size = UDim2.new(0.1,0,0.1,0)
local ToggleUI = Instance.new("TextButton", Frame)
ToggleUI.Position = UDim2.new(0,0,0,0)
ToggleUI.BackgroundColor3 = Color3.new(0,0,0)
ToggleUI.BackgroundTransparency = 1
ToggleUI.Size = UDim2.new(1,0,1,0)
ToggleUI.Text = "Toggle UI"
ToggleUI.TextSize = "20"
ToggleUI.Font = "FredokaOne"
ToggleUI.TextColor3 = Color3.new(1,1,1)
local UiStroke = Instance.new("UIStroke", Frame)
UiStroke.Color = Color3.new(1,0,0)
local UiCorner = Instance.new("UICorner", Frame)
--// Code Button \\
ToggleUI.MouseButton1Click:Connect(function()
for _,v in ipairs(game.CoreGui:GetDescendants()) do
if v:IsA("Frame") and v.Name == "TopBar" then
v.Visible = not v.Visible
end
end
end)
