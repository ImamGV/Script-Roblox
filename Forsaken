local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Bacon-Library/refs/heads/main/bacon.txt"))()
local LibESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Script/main/ESP"))()

local ScreenGui2= Instance.new("ScreenGui", game:GetService("Players").LocalPlayer.PlayerGui)
ScreenGui2.Name = "HelpGUI"
ScreenGui2.ResetOnSpawn = true
local Frame2 = Instance.new("Frame", ScreenGui2)
Frame2.Position = UDim2.new(0.875, 0, 0.3, 0)
Frame2.BackgroundColor3 = Color3.new(0,0,0)
Frame2.BackgroundTransparency = 0
Frame2.Size = UDim2.new(0.1,0,0.1,0)
local ToggleUI = Instance.new("TextButton", Frame2)
ToggleUI.Position = UDim2.new(0,0,0,0)
ToggleUI.BackgroundColor3 = Color3.new(0,0,0)
ToggleUI.BackgroundTransparency = 1
ToggleUI.Size = UDim2.new(1,0,1,0)
ToggleUI.Text = "Toggle UI"
ToggleUI.TextSize = "25"
ToggleUI.Font = "FredokaOne"
ToggleUI.TextColor3 = Color3.new(1,1,1)
local UiStroke10 = Instance.new("UIStroke", Frame2)
local Uiv = Instance.new("UICorner", Frame2)
UiStroke10.Color = Color3.new(1)
ToggleUI.MouseButton1Click:Connect(function()
for _,v in ipairs(game.CoreGui:GetDescendants()) do
if v:IsA("Frame") and v.Name == "TopBar" then
v.Visible = not v.Visible
end
end
end)

local Tabs = {
Main = Library:CreateWindow("Main"),
}
local functiom InfinityStamina(v)
Work = v
local StaminaScript = require(game.ReplicatedStorage.Systems.Character.Game.Sprinting)
InfStamina = game:GetService("RunService").Heartbeat:Connect(function()
if not Work then
InfStamina:Disconnect()
StaminaScript.StaminaLossDisabled = nil
return
end
StaminaScript.StaminaLossDisabled = function() 
end
end)
end
Library:CreateToggle(Tabs.Main, "Infinity Stamina", false, function(v)
InfinityStamina(v)
end)
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _G.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 16 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
game.Players.LocalPlayer.CharacterAdded:Connect(function()
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _G.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 16 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
end)
Library:CreateToggle(Tabs.Main, "Disabled Slow & Stun", false, function(v)
_G.NoStun = V
end)
Library:CreateToggle(Tabs.Main, "Killer ESP", false, function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v:IsA("Model") and v.Parent.Name == "Killers" and not v:FindFirstChild("Killer_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
elseif v.Parent.Name == "Killers" and not v:FindFirstChild("Killer_ESP") then
LibESP:AddESP(v.HumanoidRootPart, "Killer", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end
KillersESP = workspace.Players.DescendantAdded:Connect(function(v)
if v:IsA("Model") and v.Parent.Name == "Killers" and not v:FindFirstChild("Killers_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
elseif v.Parent.Name == "Killers" and not v:FindFirstChild("Killer_ESP") then
LibESP:AddESP(v.HumanoidRootPart, "Killer", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end)
else
KillersESP:Disconnect()
LibESP:Delete("Killers_ESP")
end
end)
Library:CreateToggle(Tabs.Main, "Survivors ESP", false, function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v:IsA("Model") and v.Parent.Name == "Survivors" and not v:FindFirstChild("Survival_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end
SurvivalESP = workspace.Players.DescendantAdded:Connect(function(v)
if v:IsA("Model") and v.Parent.Name == "Survivors" and not v:FindFirstChild("Survival_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end)
else
SurvivalESP:Disconnect()
LibESP:Delete("Survival_ESP")
end
end)
