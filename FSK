local LibESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Script/main/ESP"))()

local Window = Library:Window({
    Title = "Forsaken",
    Desc = "Forsaken on top",
    Icon = 95073582077930,
    Theme = "Dark",
    Config = {
        Keybind = Enum.KeyCode.LeftControl,
        Size = UDim2.new(0, 560, 0, 480)
    },
